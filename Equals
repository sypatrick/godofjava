public class Equals {
    public static void main(String args[]){

        String name1 = "lee";
        String name2 = "lee";
        String name3 = new String("lee");
        String name4 = new String("lee");

        boolean flag;
        boolean flag1;

        if(name1.equals(name3)){

                flag = true;
        }
        else flag = false;

        if(name1 == name3){

                flag1 = true;
        }
        else flag1 = false;

        System.out.println(flag);
        System.out.println(flag1);
    }
}
